{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "description": "Extension with customizable settings.",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
